{
  "name": "easyaa-wallet",
  "version": "1.0.0",
  "description": "Account Abstraction Wallet - Open Source Alternative to Human Wallet",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:sandbox": "wrangler pages dev dist --ip 0.0.0.0 --port 3000",
    "build": "vite build",
    "preview": "wrangler pages dev",
    "deploy": "npm run build && wrangler pages deploy",
    "deploy:prod": "npm run build && wrangler pages deploy dist --project-name easyaa-wallet",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings",
    "clean-port": "fuser -k 3000/tcp 2>/dev/null || true",
    "test": "curl http://localhost:3000",
    "git:commit": "git add . && git commit -m"
  },
  "dependencies": {
    "hono": "^4.10.7",
    "viem": "^2.21.45",
    "permissionless": "^0.2.21"
  },
  "devDependencies": {
    "@hono/vite-build": "^1.2.0",
    "@hono/vite-dev-server": "^0.18.2",
    "@cloudflare/workers-types": "^4.20250705.0",
    "vite": "^6.3.5",
    "wrangler": "^4.4.0",
    "typescript": "^5.7.3"
  }
}